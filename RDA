baseRDA <- rda(base ~ PH + Cal + Cal_actif + MO + P2O5 + CE_meq + CE , data=base2)
summary(baseRDA)
plot(baseRDA)
AVEC LA TRANSFORMATION HELLINGER:
spe_hellinger <- decostand(base, method = "hellinger")
view(base2)
my_rda <- rda(spe_hellinger ~ ., data=base2)
summary(my_rda)
plot(my_rda)
plot(my_rda, choices = c(1, 3))
plot(my_rda,display=c("sp","bp"))
plot(my_rda,display=c("sp","bp","si"))
RsquareAdj(my_rda)
anova.cca(my_rda)
anova.cca(my_rda, by = "axis")
anova.cca(my_rda, by = "terms")
